{
  "scripts": {
    "selendra:clean": "shx rm -rf node_modules/.cache; docusaurus clear selendra-wiki",
    "selendra:test": "jest --env=jsdom",
    "selendra:start": "docusaurus start selendra-wiki",
    "selendra:build": "cross-env BUILDING=true docusaurus build selendra-wiki; node selendra-wiki/postprocess",
    "selendra:serve": "serve selendra-wiki/build",
    "selendra:publish-gh-pages": "cross-env PUBLISHING=true docusaurus deploy selendra-wiki",
    "selendra:pdf": "yarn pdf --initialDocURLs \"http://localhost:3000/docs/getting-started\" --outputPDFFilename=\"selendra-wiki/static/selendra-wiki.pdf\" --coverTitle=\"selendra WIKI\"",
    "copy:translations": "cp -R selendra-wiki/translated_docs selendra-guide ",
    "publish-gh-pages": "cd selendra-wiki; docusaurus-publish",
    "write-translations": "docusaurus-write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version",
    "crowdin-upload": "crowdin --config ../crowdin-docs-v1.yaml upload sources --auto-update -b master",
    "crowdin-download": "crowdin --config ../crowdin-docs-v1.yaml download -b master",
    "selendra:crowdin:upload:source": "crowdin upload sources --config ./crowdin.yaml -b master",
    "selendra:crowdin:download:ru": "crowdin download --config ./crowdin.yaml -b master -l ru",
    "selendra:crowdin:download:zh-CN": "crowdin download --config ./crowdin.yaml -b master -l zh-CN",
    "selendra:copy-assets:ru": "shx mkdir -p selendra-wiki/i18n/ru-RU/docusaurus-plugin-content-docs/current; shx cp -R docs/assets selendra-wiki/i18n/ru-RU/docusaurus-plugin-content-docs/current",
    "selendra:copy-assets:zh-CN": "shx mkdir -p selendra-wiki/i18n/zh-CN/docusaurus-plugin-content-docs/current; shx cp -R docs/assets selendra-wiki/i18n/zh-CN/docusaurus-plugin-content-docs/current",
    "pdf": "npx mr-pdf --contentSelector=\"article\" --paginationSelector=\".pagination-nav__item--next > a\" --pdfFormat=\"A4\" --pdfMargin=\"90,60,80,60\"",
    "crowdin": "crowdin",
    "docusaurus": "docusaurus",
    "prepare": "husky install",
    "update:auctions": "node ./components/utilities/updateAuctions.js",
    "audit:links": "python ./scripts/auditLinks.py"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@crowdin/cli": "^3.9.0",
    "@docusaurus/core": "^2.2.0",
    "@docusaurus/module-type-aliases": "^2.2.0",
    "@docusaurus/plugin-client-redirects": "^2.2.0",
    "@docusaurus/preset-classic": "^2.2.0",
    "@octokit/rest": "^19.0.5",
    "@polkadot/api": "^9.8.1",
    "@polkadot/keyring": "10.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.4",
    "axios": "^1.1.3",
    "chalk": "^5.1.2",
    "cloudflare": "^2.9.1",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.2",
    "ipfs-http-client": "^57.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "katex": "^0.16.3",
    "loud-rejection": "^2.2.0",
    "mr-pdf": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^17.0.2",
    "remarkable-katex": "^1.2.1",
    "shx": "^0.3.4",
    "unist-util-visit": "2.0.1",
    "yargs": "^17.6.2"
  },
  "dependencies": {
    "@algolia/client-search": "^4.14.2",
    "@mdx-js/runtime": "^1.6.22",
    "@polkadot/util": "10.1.1",
    "@polkadot/util-crypto": "10.1.1",
    "@polkadot/wasm-crypto": "^6.2.3",
    "@polkadot/x-randomvalues": "*",
    "@types/react": "^18.0.9",
    "ansi-html": "^0.0.9",
    "ansi-regex": "^6.0.1",
    "browserslist": "^4.21.4",
    "check-peer-dependencies": "^4.1.0",
    "crowdin-cli": "^0.3.0",
    "css-what": "^6.1.0",
    "glob-parent": "^6.0.2",
    "immer": "^9.0.16",
    "node-fetch": "^3.3.0",
    "node-forge": "^1.3.1",
    "nth-check": "^2.1.1",
    "react-treebeard": "^3.2.4",
    "remark-docusaurus-tabs": "^0.2.0",
    "set-value": "^4.1.0",
    "trim": "^1.0.1",
    "typescript": "^4.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
